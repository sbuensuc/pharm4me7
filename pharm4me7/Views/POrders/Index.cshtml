 data-th="Name"@model IEnumerable<pharm4me7.Models.POrder>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th data-th="Pharmacy Name">
            @Html.DisplayNameFor(model => model.Pharmacy.Name)
        </th>
        <th data-th="Prescription Type">
            @Html.DisplayNameFor(model => model.Prescript.DispType)
        </th>
        <th data-th="Date Ordered">
            @Html.DisplayNameFor(model => model.DateOrdered)
        </th>
        <th data-th="Note">
            @Html.DisplayNameFor(model => model.Note)
        </th>
        <th data-th="Fill">
            @Html.DisplayNameFor(model => model.Fill)
        </th>
        <th data-th="Deny">
            @Html.DisplayNameFor(model => model.Deny)
        </th>
        <th data-th="Accept">
            @Html.DisplayNameFor(model => model.Accept)
        </th>
        <th data-th="Action"></th>
    </tr>

    @foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Pharmacy.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Prescript.DispType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOrdered)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Note)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fill)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Deny)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accept)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.POrderId }) |
            @Html.ActionLink("Details", "Details", new { id=item.POrderId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.POrderId })
        </td>
    </tr>
}

</table>
