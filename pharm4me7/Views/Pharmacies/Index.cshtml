@model IEnumerable<pharm4me7.Models.Pharmacy>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th data-th="Name">
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th data-th="Address">
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th data-th="City">
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th data-th="Name">
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th data-th="Action"></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PharmacyId }) |
            @Html.ActionLink("Details", "Details", new { id=item.PharmacyId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PharmacyId })
        </td>
    </tr>
}

</table>
